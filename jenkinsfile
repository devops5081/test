pipeline {
    agent any
    tools {
        maven 'maven'
    }

    stages {
        stage('copy from git') {
            steps {
                git branch: 'main', url: 'https://github.com/devops5081/test.git'
            }
        }
        stage('clean') {
            steps {
                sh 'mvn clean'
            }
        }
        stage('package') {
            steps {
               sh 'mvn package'
            }
        }
    }
}
